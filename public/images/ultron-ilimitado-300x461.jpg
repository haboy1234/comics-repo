                var ready = [];
                var last_triggered_px = [];
                var refresh_count = [];
                refreshAtPx = function (zone, px, timeout, max_refresh) {
                    console.log('registered trigger');
                    ready[zone] = true;
                    last_triggered_px[zone] = 0;
                    refresh_count[zone] = 0;
                    var waypoint = new Waypoint({
                        element: document.getElementById('body-anchor'),
                        handler: function(direction) {
                            if (ready[zone] && last_triggered_px[zone] != px && refresh_count[zone] < max_refresh) {

                                //refresh logic goes here
                                
                                var element = document.getElementsByClassName(zone)[0];
                                iframe = element.getElementsByTagName('iframe')[0];

                                // Force reload!
                                if ( typeof iframe !== 'undefined' ) {
                                    iframe.contentDocument.location.reload(true);
                                }
                                
                                
                                refresh_count[zone]++;

                                console.log(zone + ' zone ' + px +'px triggered ' + refresh_count[zone] + 'time(s)');

                                last_triggered_px[zone] = px;
                                ready[zone] = false;

                                console.log(zone + ' not ready');
                                setTimeout(function () {
                                    ready[zone] = true;
                                    console.log(zone + ' ready');
                                }, timeout);
                            }
                        },
                        offset: ((px)* -1)
                    })
                }
            </script>
